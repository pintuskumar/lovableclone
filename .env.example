# Vercel AI Gateway API Key
# Create a key in Vercel AI Gateway (docs): https://examples.vercel.com/docs/ai-gateway
VERCEL_AI_GATEWAY_API_KEY=your_vercel_ai_gateway_api_key_here

# Optional: override the default gateway model
AI_GATEWAY_MODEL=anthropic/claude-sonnet-4.5

# Daytona API Key
# Get your key from: https://www.daytona.io/
DAYTONA_API_KEY=your_daytona_api_key_here

# App runtime secrets (required for lovable-ui auth/database features)
DATABASE_URL=postgresql://postgres:password@localhost:5432/lovable_clone
JWT_SECRET=replace_with_a_secure_random_secret
